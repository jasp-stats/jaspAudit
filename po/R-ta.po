msgstr "மக்கள்தொகை அளவு"

msgid "Absolute value"
msgstr "முழுமையான மதிப்பு"

msgid "Mean"
msgstr "இடை, சராசரி"

msgid "Std. deviation"
msgstr "Std. விலகல்"

msgid "1"
msgstr "1"

msgid "Quartile"
msgstr "காலாண்டு"

msgid "2"
msgstr "2"

