msgstr "Bilateral"

msgctxt "IntervalType|"
msgid "Produces a two-sided interval for the misstatement in the population."
msgstr "Produce un intervalo bilateral para a inexactitude na poboación."

msgctxt "IntervalType|"
msgid "Lower bound (> materiality)"
msgstr "Límite inferior (> Materialidade)"

msgctxt "IntervalType|"
msgid "Lower bound"
msgstr "Límite inferior"

msgctxt "IntervalType|"
